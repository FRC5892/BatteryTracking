[Unit]
Description=Service that runs Battery Tracking

[Service]
WorkingDirectory=/opt/batteryTracking
ExecStart=/usr/bin/java -jar /opt/batteryTracking/BatteryTracking.jar <TEAM NUMBER>
ExecStop=/bin/systemctl kill BatteryTracking
Type=simple
Restart=on-failure
RestartSec=1

[Install]
WantedBy=multi-user.target
